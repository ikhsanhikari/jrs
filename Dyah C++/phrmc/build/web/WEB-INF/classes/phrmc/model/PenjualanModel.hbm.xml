<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="phrmc.model.PenjualanModel" table="tb_Penjualan">
        <id column="Kode_Penjualan" name="kode">
            <generator class="assigned"/>
        </id>
        <property name="id_obat" column="ID_Obat"/>
        <property name="tgl" column="Tanggal_Transaksi"/>
        <property name="id_user" column="ID_User"/>
        <property name="harga" column="Harga"/>
        <property name="jumlah" column="Jumlah"/>
        <property name="tot" column="Total"/>
  
    </class>
</hibernate-mapping>
