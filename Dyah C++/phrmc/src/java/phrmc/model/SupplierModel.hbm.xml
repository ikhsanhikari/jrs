<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="phrmc.model.SupplierModel" table="tb_supplier">
        <id name="id" column="Id_Supplier">
            <generator class="assigned"/>
        </id>
        <property name="nama" column="Nama"/>
        <property name="alamat" column="Alamat"/>
        <property name="telp" column="Telepon"/>
        <property name="email" column="Email"/>
    </class>
  

</hibernate-mapping>
