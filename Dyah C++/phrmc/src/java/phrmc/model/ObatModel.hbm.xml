<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="phrmc.model.ObatModel" table="tb_obat">
        <id name="kode" column="Kode_Obat">
            <generator class="assigned"/>
        </id>
        <property name="nama" column="Nama_Obat"/>
        <property name="tgl" column="Tanggal_Kadaluarsa"/>
        <property name="stok" column="Stok"/>
        <property name="harga" column="Harga"/>
        <property name="kategori" column="Kategori"/>
    </class>
  

</hibernate-mapping>
